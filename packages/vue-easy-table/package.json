    "@vitest/coverage-istanbul": "0.29.2",
    "@vue/test-utils": "2.3.0",
    "@vueuse/core": "9.12.0",
    "eslint": "8.34.0",
    "eslint-config-custom": "0.7.4",
    "jsdom": "21.1.0",
    "prettier": "2.8.4",
    "tsconfig-custom": "0.7.4",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.2",
    "vue": "3.2.47",
    "vue-tsc": "1.2.0"
  },
  "peerDependencies": {
    "vue": ">=3.2"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
