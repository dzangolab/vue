{
  "name": "@dzangolab/vue-config",
  "version": "0.0.13",
  "description": "Vue config plugin",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/DzangolabVueConfig.es.js",
      "require": "./dist/DzangolabVueConfig.umd.js"
    }
  },
  "main": "dist//DzangolabVueConfig.umd.js",
  "module": "dist//DzangolabVueConfig.es.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "sort-package": "npx sort-package-json",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "devDependencies": {
    "@types/node": "18.11.17",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "eslint": "8.30.0",
    "eslint-config-custom": "0.0.13",
    "prettier": "2.8.1",
    "tsconfig": "0.0.13",
    "typescript": "4.9.4",
    "vite": "4.0.1",
    "vue": "3.2.45",
    "vue-tsc": "1.0.13"
  },
  "peerDependencies": {
    "vue": ">=3.2"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}