{
  "name": "@dzangolab/vue3-user",
  "version": "0.6.0",
  "description": "Vue User Plugin",
  "exports": {
    ".": {
      "import": "./dist/DzangolabVue3User.es.js",
      "require": "./dist/DzangolabVue3User.umd.js"
    },
    "./dist/DzangolabVue3User.css": "./dist/style.css"
  },
  "main": "dist//DzangolabVue3User.umd.js",
  "module": "dist//DzangolabVue3User.es.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "sort-package": "npx sort-package-json",
    "test": "vitest --environment jsdom run --coverage",
    "test:component": "vitest --environment jsdom run component/",
    "test:snapshot": "vitest --environment jsdom run snapshot/",
    "test:unit": "vitest --environment jsdom run unit/",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false"
  },
  "dependencies": {
    "@dzangolab/vue3-config": "0.6.5",
    "@dzangolab/vue3-i18n": "0.6.5",
    "@dzangolab/vue3-layout": "0.6.5",
    "@dzangolab/vue3-ui": "0.6.5",
    "supertokens-web-js": "0.4.0",
    "supertokens-website": "15.0.0"
  },
  "devDependencies": {
    "@dzangolab/vue3-config": "0.6.5",
    "@iconify/vue": "4.1.0",
    "@types/node": "18.11.19",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "@vitest/coverage-istanbul": "0.28.4",
    "@vue/test-utils": "2.2.10",
    "@vueuse/core": "9.12.0",
    "axios": "1.2.3",
    "eslint": "8.33.0",
    "eslint-config-custom": "0.6.5",
    "pinia": "2.0.29",
    "prettier": "2.8.3",
    "tsconfig": "0.6.5",
    "typescript": "4.9.5",
    "vee-validate": "4.7.3",
    "vue-router": "4.1.6",
    "vite": "4.1.1",
    "vitest": "0.28.4",
    "vue": "3.2.45",
    "vue-tsc": "1.0.24",
    "yup": "0.32.11"
  },
  "peerDependencies": {
    "@vee-validate/i18n": ">=4.7.1",
    "@vee-validate/rules": ">=4.7.1",
    "@vueuse/core": ">=9.4.0",
    "axios": ">=0.26.1",
    "pinia": ">=2.0",
    "vee-validate": ">=4.7.1",
    "vue": ">=3.2",
    "vue-router": ">=4.0",
    "yup": ">=0.32.11"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}