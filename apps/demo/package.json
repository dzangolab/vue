{
  "name": "@dzangolab/vue3-demo",
  "version": "0.6.5",
  "private": true,
  "scripts": {
    "build": "run-p type-check build-only",
    "dev": "vite",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@dzangolab/vue3-config": "0.6.5",
    "@dzangolab/vue3-i18n": "0.6.5",
    "@dzangolab/vue3-layout": "0.6.5",
    "@dzangolab/vue3-ui": "0.6.5",
    "@kyvg/vue3-notification": "2.8.0",
    "pinia": "2.0.29",
    "vue": "3.2.47",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@types/jsdom": "20.0.1",
    "@types/node": "18.13.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitejs/plugin-vue-jsx": "3.0.0",
    "@vitest/coverage-istanbul": "0.28.4",
    "@vue/test-utils": "2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "8.33.0",
    "eslint-config-custom": "0.6.5",
    "eslint-plugin-cypress": "^2.12.1",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.8.4",
    "tsconfig": "0.6.5",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.4",
    "vue-tsc": "1.0.24"
  }
}
